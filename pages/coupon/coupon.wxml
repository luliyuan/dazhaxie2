<!--pages/order/order.wxml-->
<!-- <view class="header">海间北岛餐厅</view> -->
<view style="display: flex;flex-direction: column;height: 100%;">
	<view style="height: 42px;">
		<i-tabs current="{{ current }}" i-class="tab" color="#ff4200" bindchange="handleChange">
			<i-tab key="1" title="未使用"></i-tab>
			<i-tab key="2" title="已使用"></i-tab>
			<i-tab key="3" title="已过期"></i-tab>
		</i-tabs>
	</view>
	

	<view class="coupon">

		<view wx:if="{{vouchers.length}}" wx:for="{{vouchers}}" class="coupon_list" style="background: url(data:image/png;base64,) no-repeat; background-size: cover;">
			<view class="coupon_price">
				<view class="price"><text>{{item.price}}</text><text>元</text></view>
				<view class="info">满{{item.payMoney}}减{{item.price}}</view>
			</view>
			<view class="coupon_info">
				<text>{{item.title}}</text>
				<text wx:if="{{item.label==1}}">{{item.content}}(仅外卖使用) </text>
				<text wx:elif="{{item.label==2}}">{{item.content}}(仅活物使用) </text>
				<text wx:elif="{{item.label==3}}">{{item.content}}(仅冰鲜使用) </text>
				<text>使用期限至:{{item.endTime}}</text>
			</view>
		</view>

	</view>
	<view wx:if="{{!vouchers.length}}" style="flex:1;background: #FFFFFF;">
		<image mode="aspectFit" style="width:134px;height:134px;margin-left:auto;margin-right:auto;display:block;margin-top: 300rpx;" src="../../static/img/nozhuan.png"></image>
		<view style="font-size:15px;color:#949494;line-height:33px;text-align:center;margin-top: 30rpx;">暂无优惠券</view>
	</view>
</view>
